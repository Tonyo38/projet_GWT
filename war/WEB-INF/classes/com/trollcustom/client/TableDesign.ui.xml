<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel width="70%" styleName="tablePanel">
		<g:Label text="Tableau de trolls" styleName="titre"/>
		<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
			<g:FlowPanel ui:field="flowPanel" width="100%" height="100%"/>
			<g:HorizontalPanel>
				<g:Button styleName="button" ui:field="buttonRetour">Retour</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 